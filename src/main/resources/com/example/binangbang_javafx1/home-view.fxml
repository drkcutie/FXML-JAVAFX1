<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ColorPicker?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<AnchorPane fx:id="paneDashBoard" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.binangbang_javafx1.HelloController">
   <children>
      <VBox alignment="TOP_CENTER" prefHeight="200.0" prefWidth="100.0">
         <children>
            <ColorPicker fx:id="cpPicker" />
            <Button fx:id="btnLogout" mnemonicParsing="false" onAction="#btnLogout" onMouseClicked="#userLogout" text="Logout" />
         </children>
      </VBox>
   </children>
</AnchorPane>
